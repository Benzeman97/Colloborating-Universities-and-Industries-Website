<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
   <class name="com.benz.demo.Event" table="Event">
   
   <id name="reg_no" column="E_id"/>
   
   <property name="orga_name" column="Orga_Name"/>
   <property name="event_name" column="EventName"/>
   
   <property name="date" column="U_Date"/>
   <property name="details" column="Infor"/>
   
   <property name="photo" column="E_Photo"/>
   
   <property name="uni_name" column="University"/>
   
   </class>
</hibernate-mapping>
