<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping >
   <class name="com.benz.demo.Sidea" table="Student_Idea">
   
   <id name="id" column="Id"/>
   <property name="uName" column="U_name"/>
      <property name="sName" column="S_name"/>
   
      <property name="title" column="Degree_Title"/>
      <property name="date" column="U_Date"/>
   
      <property name="idea" column="Idea"/>
      <property name="mail" column="Email"/>
   
      <property name="gen" column="Gender"/>
      
      <property name="company" column="Company"/>
   
   </class>
</hibernate-mapping>
